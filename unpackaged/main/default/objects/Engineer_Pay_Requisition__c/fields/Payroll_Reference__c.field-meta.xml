 ISNULL(Payroll_month__c),
 &quot;&quot;, 
 RIGHT(TEXT(YEAR(Payroll_month__c)),2)  &amp;&quot;/&quot;&amp; LPAD(TEXT(MONTH(Payroll_month__c)),2,&quot;0&quot;)  
)</formula>
