<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mobile_Phone_Cannot_be_Blank</fullName>
    <active>false</active>
    <errorConditionFormula>ISBLANK( Mobile_No__c )</errorConditionFormula>
    <errorMessage>Every Service Resource MUST have a mobile phone number or many processes will break</errorMessage>
</ValidationRule>
