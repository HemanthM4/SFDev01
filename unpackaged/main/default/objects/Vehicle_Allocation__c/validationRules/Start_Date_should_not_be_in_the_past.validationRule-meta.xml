<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Date_should_not_be_in_the_past</fullName>
    <active>false</active>
    <errorConditionFormula>Start_date__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Start_date__c</errorDisplayField>
    <errorMessage>The Start date must be equal or greater than today</errorMessage>
</ValidationRule>
