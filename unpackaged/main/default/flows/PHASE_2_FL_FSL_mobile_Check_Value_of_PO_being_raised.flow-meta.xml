        <locationX>613</locationX>
        <locationY>241</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Exceed_total_of_PREQ_incl_other_POs</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_Value_Of_PO_With_VAT</leftValueReference>
                <operator>GreaterThan</operator>
