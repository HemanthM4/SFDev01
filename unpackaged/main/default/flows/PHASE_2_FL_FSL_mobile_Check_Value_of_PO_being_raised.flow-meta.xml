            </conditions>
            <connector>
                <targetReference>ScreenValue_of_PO_Exceeds_Spare_Money_for_POs</targetReference>
            </connector>
            <label>Exceed total of PREQ incl. other POs</label>
        </rules>
        <rules>
            <name>Does_NOT_Exceed_total_of_PREQ_incl_other_POs</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_Value_Of_PO_With_VAT</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>var_SpareToSpendOnPOs</elementReference>
                </rightValue>
            </conditions>
            <label>Does NOT Exceed total of PREQ incl. other POs</label>
        </rules>
    </decisions>
