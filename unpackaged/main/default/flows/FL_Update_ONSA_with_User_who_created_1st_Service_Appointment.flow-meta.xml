    <decisions>
        <name>Check_for_On_Site_Agr_record</name>
        <label>Check for On Site Agr record</label>
        <locationX>761</locationX>
        <locationY>39</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>No_ONSA</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>var_WorkOrderData.On_site_agreement__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_WorkOrderData.On_site_agreement__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <label>No ONSA</label>
        </rules>
        <rules>
            <name>Got_ONSA</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_WorkOrderData.On_site_agreement__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_WorkOrderData.On_site_agreement__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_ONSA_Data</targetReference>
            </connector>
            <label>Got ONSA</label>
        </rules>
    </decisions>
