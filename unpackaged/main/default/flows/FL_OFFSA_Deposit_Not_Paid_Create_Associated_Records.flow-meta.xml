    <decisions>
        <name>Is_there_already_a_WO</name>
        <label>Is there already a WO?</label>
        <locationX>578</locationX>
        <locationY>66</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_got_WO</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_OFFSAData.Work_Order__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_OFFSAData.Work_Order__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ScreenAlready_have_WO</targetReference>
            </connector>
            <label>Yes - got WO</label>
        </rules>
        <rules>
            <name>No_Work_Order_Exists</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>var_OFFSAData.Work_Order__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_OFFSAData.Work_Order__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Screen1</targetReference>
            </connector>
            <label>No Work Order Exists</label>
        </rules>
    </decisions>
    <formulas>
        <name>FormulaDetailsUserWhoClickedButton</name>
        <dataType>String</dataType>
        <expression>{!$User.Username} &amp; &quot; Date: &quot; &amp; TEXT({!$Flow.CurrentDateTime})</expression>
    </formulas>
    <formulas>
        <name>FormulaReplaceWorldPayDepositIDBankTransfer</name>
        <dataType>String</dataType>
        <expression>LEFT({!$User.FirstName} &amp; &quot; &quot; &amp; {!$User.LastName} &amp; &quot; bypassed Deposit process as paid via Bank Transfer&quot;,255)</expression>
    </formulas>
    <formulas>
        <name>FormulaReplaceWorldPayDepositIDCreditCustomer</name>
        <dataType>String</dataType>
        <expression>LEFT({!$User.FirstName} &amp; &quot; &quot; &amp; {!$User.LastName} &amp; &quot; bypassed Deposit process as Credit Customer&quot;,255)</expression>
    </formulas>
