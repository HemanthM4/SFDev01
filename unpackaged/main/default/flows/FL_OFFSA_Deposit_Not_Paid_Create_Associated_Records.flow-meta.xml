    <decisions>
        <name>check_if_deposit_paid_in_full</name>
        <label>check if deposit paid in full</label>
        <locationX>1370</locationX>
        <locationY>59</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Deposit_paid_in_full</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ChoicePaidFullDeposit</leftValueReference>
                <operator>WasSelected</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_OFFSA</targetReference>
            </connector>
            <label>Deposit paid in full</label>
        </rules>
        <rules>
            <name>Deposit_not_paid_in_full</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ChoiceNotPaidDepositInFull</leftValueReference>
                <operator>WasSelected</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ScreenNot_Paid_Deposit_in_full</targetReference>
            </connector>
            <label>Deposit not paid in full</label>
        </rules>
    </decisions>
