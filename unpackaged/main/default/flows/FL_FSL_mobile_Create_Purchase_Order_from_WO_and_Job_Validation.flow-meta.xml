                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Visit Complete</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_ServiceApptData.Status</leftValueReference>
                <operator>EqualTo</operator>
