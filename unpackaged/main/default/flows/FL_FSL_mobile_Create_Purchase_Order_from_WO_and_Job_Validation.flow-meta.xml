        </rules>
        <rules>
            <name>Status_is_Dispatched_or_In_Transit_or_On_Site</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>var_ServiceApptData.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Dispatched</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_ServiceApptData.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>In Transit</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_ServiceApptData.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>On site</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Call_FL_FSL_mobile_Create_Purchase_Order_from_WO_and_Job_update_CandCs</targetReference>
            </connector>
            <label>Status is Dispatched or In Transit or On Site</label>
        </rules>
